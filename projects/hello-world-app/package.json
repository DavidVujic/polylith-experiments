{
  "name": "hello-world-app",
  "version": "0.0.1",
  "scripts": {
    "clean": "rm -rf ./deploy",
    "copy:css": "cp -r ../../bases/views/public/hello_world/css ./deploy",
    "copy:html": "cp ../../bases/views/public/hello_world/index.html ./deploy",
    "copy": "npm run copy:css && npm run copy:html",
    "shadow:release": "shadow-cljs release app",
    "release": "npm run clean && npm run shadow:release && npm run copy"
  },
  "devDependencies": {
    "shadow-cljs": "2.12.1"
  },
  "dependencies": {
    "react": "17.0.1",
    "react-dom": "17.0.1"
  }
}
